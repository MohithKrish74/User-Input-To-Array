package com.company;

import java.util.*;

public class Main
{
    public static void main(String[] args)
    {
        System.out.print("Enter the size of the array: ");
        Scanner in =new Scanner(System.in);
        int n=in.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=in.nextInt();
        }
        System.out.println(Arrays.toString(a));
    }

}


